{
  "manifest_version": 3,
  "name": "Computer club: Startpagina",
  "description": "Startpagina voor ComputerClub. Een wekelijkse podcast over technologie.",
  "version": "0.0.0.1",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": ["https://computerclub.forum/*", "https://feeds.soundcloud.com/*"]
}